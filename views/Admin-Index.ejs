            <h1>Edit Exercise</h1>
            <form action="/auth/editexercise" method="post" id="editExerciseForm">
                <div class="form-group">
                    <label for="editExerciseName">Current Exercise Name:</label>
                    <input type="text" id="editExerciseName" name="editExerciseName" required>
                    <div id="editExerciseNameError" class="error-message"></div>
                </div>

                <div class="form-group">
                    <label for="newExerciseName">New Name:</label>
                    <input type="text" id="newExerciseName" name="newExerciseName" required>
                    <div id="newExerciseNameError" class="error-message"></div>
                </div>

                <div class="form-group">
                    <label for="exercisedescription">Exercise Description:</label>
                    <input type="text" id="exercisedescription" name="exercisedescription" required>
                </div>

                <div class="form-group">
                    <label for="exerciseimage">Exercise Image URL:</label>
                    <input type="text" id="exerciseimage" name="exerciseimage" required>
                </div>
                <button type="submit" class="btn-action">Edit Exercise</button>
                <div id="editExerciseSuccessMessage" class="success-message"></div>
            </form>
        </div>


        <!--  Add admin Buttons -->
        <div class="action-container hidden" id="addAdminContainer">
            <button class="exit-button" onclick="toggleVisibility('addAdminContainer')">X</button>
            <h1>Add Admin</h1>
            <form action="/auth/addAdmin" method="post" id="addAdminForm" enctype="multipart/form-data">
                <div class="form-group">
                    <label for="AdminName">Admin Name:</label>
                    <input type="text" id="AdminName" name="AdminName" required>
                    <div id="AdminNameError" class="error-message"></div>
                </div>
                <div class="form-group">
                    <label for="AdminPrice">Admin Price:</label>
                    <input type="number" id="AdminPrice" name="AdminPrice" required>
                    <div id="AdminPriceError" class="error-message"></div>
                </div>
                <div class="form-group">
                    <label for="AdminId">Admin ID:</label>
                    <input type="number" id="AdminId" name="AdminId" required>
                    <div id="AdminIdError" class="error-message"></div>
                </div>
                <div class="form-group">
                    <label for="AdminImage">Admin Image:</label>
                    <input type="file" id="AdminImage" name="AdminImage" accept="image/*" required>
                </div>
                <button type="submit" class="btn-action">Add Admin</button>
                <div id="successMessage" class="success-message"></div>
            </form>
        </div>


     
